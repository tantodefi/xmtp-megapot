# XMTP Agent Configuration
# Generate keys using: yarn gen:keys

# Private key for the wallet (EOA)
WALLET_KEY=0x...

# Encryption key for local database
ENCRYPTION_KEY=...

# XMTP Environment: local, dev, or production
XMTP_ENV=dev

# OpenAI API Key for smart message processing
OPENAI_API_KEY=your_openai_api_key_here

# Neynar API Key for Farcaster display name resolution (optional)
# Get your API key from https://neynar.com 
# When configured, agent will resolve Farcaster usernames for personalized greetings
# Example: 0x1234...5678 becomes "dwr.eth" or "vitalik"
NEYNAR_API_KEY=your_neynar_api_key_here

# MegaPot Contract Configuration
MEGAPOT_CONTRACT_ADDRESS=0xbEDd4F2beBE9E3E636161E644759f3cbe3d51B95
MEGAPOT_USDC_ADDRESS=0x833589fCD6eDb6E08f4c7C32D4f71b54bdA02913
MEGAPOT_REFERRER_ADDRESS=0xa14ce36e7b135b66c3e3cb2584e777f32b15f5dc

# JackpotPool Contract for Group Pooled Purchases
JACKPOT_POOL_CONTRACT_ADDRESS=0xfb324c09c16b5f437ff612a4e8bc95b8fd6e6d5a

# MegaPot API Configuration (optional)
# Get your API key from MegaPot telegram group
MEGAPOT_DATA_API_KEY=your_megapot_data_api_key_here

# Base Network RPC URL
