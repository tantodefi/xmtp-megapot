    "agent",
    "megapot",
    "lottery",
    "blockchain",
    "base",
    "inline-actions",
    "agent-sdk"
  ],
  "type": "module",
  "scripts": {
    "build": "npx tsc && echo 'âœ… TypeScript compilation successful'",
